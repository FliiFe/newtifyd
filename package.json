{
    "name": "newtifyd",
    "version": "1.0.0",
    "description": "A modern, slick notification daemon",
    "main": "lib",
    "scripts": {
        "start": "electron lib | tee /tmp/newtify.log",
        "buildback": "rm -rf lib && npx babel src -d lib",
        "buildfront": "cd front && npm run build",
        "build": "npm run buildback && npm run buildfront",
        "rebuild-modules": "npx electron-rebuild",
        "nodemon": "nodemon -i lib -x 'npm run buildback && npm run start'"
    },
    "author": "Th√©ophile Cailliau (FliiFe)",
    "license": "MIT",
    "homepage": "https://github.com/FliiFe/newtifd",
    "repository": "github:FliiFe/newtifd",
    "dependencies": {
        "chalk": "^4.0.0",
        "cheerio": "^1.0.0-rc.2",
        "datauri": "^3.0.0",
        "dbus-next": "^0.8.2",
        "express": "^4.16.4",
        "file-type": "^14.5.0",
        "flatten": "^1.0.2",
        "loglevel": "^1.6.1",
        "loglevel-plugin-prefix": "^0.8.4",
        "mz": "^2.7.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.3",
        "@babel/core": "^7.4.3",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-decorators": "^7.8.3",
        "@babel/preset-env": "^7.4.3",
        "babel-eslint": "^10.1.0",
        "babel-register": "^6.26.0",
        "electron": "^9.0.0",
        "electron-rebuild": "^1.11.0",
        "eslint": "^7.0.0"
    }
}
